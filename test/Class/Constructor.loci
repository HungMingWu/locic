// Test output.
void testPrint(const char * const str, ...);

// Class definition.
class TestClass(int value) {
	static Create(int value) {
		testPrint("TestClass.Create: Value = %d", value);
		return @(value);
	}
	
	static Other(int value) {
		testPrint("TestClass.Other: Value = %d", value);
		return @(value);
	}
}

// Entry point.
int testEntryPoint(int argc, char ** argv) {
	(void) TestClass(1);
	(void) TestClass::Create(2);
	(void) TestClass::Other(3);
	return 0;
}

