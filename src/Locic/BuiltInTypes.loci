__primitive null_t{ };

__primitive bool{
	bool not();
};

__primitive char{
	char implicitCopy();
	
	int compare(char v);
	
	bool isZero();
};

__primitive short{
	short implicitCopy();
	
	short abs();

	short add(short v);
	short subtract(short v);
	short multiply(short v);
	short divide(short v);
	short modulo(short v);
	
	int compare(short v);
};

__primitive int{
	int implicitCopy();
	
	int abs();

	int add(int v);
	int subtract(int v);
	int multiply(int v);
	int divide(int v);
	int modulo(int v);
	
	int compare(int v);
	bool isZero();
	bool isPositive();
	bool isNegative();
};

__primitive long{
	long implicitCopy();
	
	long abs();

	long add(long v);
	long subtract(long v);
	long multiply(long v);
	long divide(long v);
	long modulo(long v);
	
	int compare(long v);
};

__primitive float{
	float implicitCopy();

	float plus(float v);
};

__primitive double{
	double implicitCopy();
	
	double plus(double v);
};


