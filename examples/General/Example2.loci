

class TestClass(const char * const n, int i, int j){
	static Null() {
		return @("NULL", 0, 0);
	}
	
	static Create(int v) {
		return @("NON-NULL", v, v + 1);
	}
	
	~ {
		printf("Destroying TestClass(n: %s, i: %d, j: %d).\n",
			@n, @i, @j);
	}

	int getValue() {
		return 42;
	}
	
	void doSomething() { }
}

