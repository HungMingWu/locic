// Test output.
import void testPrint(const char * const str, ...);

exception ExampleEmptyError();

// Entry point.
export int testEntryPoint(int argc, char ** argv) {
	try {
		throw ExampleEmptyError();
	} catch (ExampleEmptyError error) {
		testPrint(C"Caught empty exception!");
	}
	return 0;
}

