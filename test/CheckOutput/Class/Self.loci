// Test output.
import void testPrint(const byte * const str, ...);

// Class definition.
class TestClass(int value) {
	static Create = default;
	
	void printValue() {
		testPrint(C"TestClass.printValue: Value = %d", @value);
	}
	
	void printValueTwice() {
		self.printValue();
		self.printValue();
	}
	
}

// Entry point.
export int testEntryPoint(int argc, byte ** argv) {
	auto testObject = TestClass(1011);
	testObject.printValueTwice();
	return 0;
}

