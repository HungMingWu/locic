// Test output.
import void testPrint(const byte * const str, ...);

int testVal(int value) {
	if (value > 10) {
		return value - 1;
	} else {
		return value + 1;
	}
}

// Entry point.
export int testEntryPoint(int argc, byte ** argv) {
	int i = 8;
	
	testPrint(C"testEntryPoint: START");
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	i = testVal(i);
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	i = testVal(i);
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	i = testVal(i);
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	i = testVal(i);
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	i = testVal(i);
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	i = testVal(i);
	
	testPrint(C"testEntryPoint: i = %d", i);
	
	testPrint(C"testEntryPoint: END");
	
	return 0;
}

