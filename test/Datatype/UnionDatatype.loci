// Test output.
void testPrint(const char * const str, ...);

datatype UnionDataType =
	TestDatatype(int a) |
	Test2Datatype(int a, int b);

TestDatatype makeData() {
	return TestDatatype(12345);
}

void printData(UnionDataType data) {
	switch (data) {
		case TestDatatype(int a) {
			testPrint("printData: TestDatatype(a = %d)"C, a);
		}
		case Test2Datatype(int a, int b) {
			testPrint("printData: Test2Datatype(a = %d, b = %d)"C, a, b);
		}
	}
}

// Entry point.
int testEntryPoint(int argc, char ** argv) {
	TestDatatype data = makeData();
	
	testPrint("testEntryPoint: data = TestDatatype(a = %d)"C, data.a);
	
	printData(data);
	
	printData(Test2Datatype(4, 5));
	
	return 0;
}

