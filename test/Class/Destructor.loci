// Test output.
void testPrint(const char * const str, ...);

// Class definition.
class TestClass(int value) {
	static Create(int value) {
		testPrint("TestClass.Create: Value = %d", value);
		return @(value);
	}
	
	~ {
		testPrint("TestClass.~: Value = %d", @value);
	}
	
}

// Entry point.
int testEntryPoint(int argc, char ** argv) {
	auto testInstance1 = TestClass(1);
	auto testInstance2 = TestClass(2);
	
	{
		(void) TestClass(3);
		auto testInstance3 = TestClass(4);
	}
	
	auto testInstance5 = TestClass(5);
	
	return 0;
}

