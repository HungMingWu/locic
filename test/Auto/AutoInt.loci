// Test output.
import void testPrint(const char * const str, ...);

// Entry point.
export int testEntryPoint(int argc, char ** argv) {
	int i = 4;
	
	testPrint(C"testEntryPoint: i = %d.", i);
	
	auto j = i.abs() + 2;
	
	testPrint(C"testEntryPoint: i = %d, j = %d.", i, j);
	
	auto k = j;
	
	testPrint(C"testEntryPoint: i = %d, j = %d, k = %d.", i, j, k);
	
	i = 1;
	
	testPrint(C"testEntryPoint: i = %d, j = %d, k = %d.", i, j, k);
	
	j = 2;
	
	testPrint(C"testEntryPoint: i = %d, j = %d, k = %d.", i, j, k);
	
	k = 3;
	
	testPrint(C"testEntryPoint: i = %d, j = %d, k = %d.", i, j, k);
	
	return 0;
}

